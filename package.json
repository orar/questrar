{
  "name": "questrar",
  "version": "2.0.0",
  "description": "A simple React request state manager and request reporting",
  "main": "dist/questrar.js",
  "unpkg": "dist/questrar.min.js",
  "module": "esm/questrar-esm.js",
  "jsnext:main": "esm/questrar-esm.js",
  "author": "Bastin George",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "bugs": {
    "url": "https://github.com/orarr/questrar/issues"
  },
  "files": [
    "dist",
    "esm",
    "redux",
    "lib",
    "src",
    "*.d.ts"
  ],
  "scripts": {
    "lint": "eslint ./",
    "clean": "rimraf ./dist ./esm ./lib",
    "test:local": "jest",
    "test": "cross-env CI=1 jest",
    "build": "rollup -c",
    "build:flow": "yarn flow-remove-types src/ -d es/ -q -a -p",
    "start": "rollup -c -w",
    "prepare": "yarn clean && yarn build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "prepublish": "yarn prepare",
    "story": "start-storybook -p 9001 -c .storybook"
  },
  "peerDependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "externals": {
    "prop-types": "PropTypes",
    "react": "React",
    "react-dom": "ReactDom"
  },
  "dependencies": {
    "invariant": "^2.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/orarr/questrar.git"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^5.0.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-flow": "^6.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "chai-enzyme": "^1.0.0-beta.1",
    "chai-shallow-deep-equal": "^1.4.6",
    "cross-env": "^5.2.0",
    "cssnano": "^4.1.4",
    "dirty-chai": "^2.0.1",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.76.0",
    "flow-remove-types": "^1.2.3",
    "flux-standard-action": "^2.0.3",
    "gh-pages": "^2.0.1",
    "hoist-non-react-statics": "^3.2.0",
    "jest": "^23.6.0",
    "jest-enzyme": "^7.0.0",
    "lodash": "^4.17.10",
    "node-sass": "^4.9.3",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.6.3",
    "react-test-renderer": "^16.6.0",
    "recompose": "^0.30.0",
    "redux": "^4.0.0",
    "redux-mock-store": "^1.5.3",
    "requirejs": "^2.3.6",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.4",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-size-snapshot": "^0.7.0",
    "rollup-plugin-uglify": "^6.0.0",
    "rollup-plugin-url": "^2.0.0",
    "sass": "^1.14.0",
    "sinon": "^6.3.4",
    "sinon-chai": "^3.2.0",
    "typescript": "^2.9.2"
  },
  "keywords": [
    "javascript",
    "react",
    "react-component",
    "request-state",
    "request-reporting"
  ]
}
